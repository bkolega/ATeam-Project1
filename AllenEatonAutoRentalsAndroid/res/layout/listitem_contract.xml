<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/list_contract_customer_name"
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:text="" />

    <TextView
        android:id="@+id/list_contract_customer_email"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/list_contract_cardnumber"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/list_contract_car_id"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/list_contract_car_type"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/list_contract_car_make"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/list_contract_car_model"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/list_contract_car_year"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/list_contract_license_plate"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/list_contract_license_state"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/list_contract_employee_email"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/list_contract_start_date"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/list_contract_end_date"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/list_contract_city"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/list_contract_state"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/list_contract_per_week"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="fill_horizontal"
        android:gravity="start"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/list_contract_gps"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/list_contract_ktag"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/list_contract_child_seat"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/list_contract_assistance"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/list_contract_ainsurance"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/list_contract_dinsurance"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/list_contract_checkin_date"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="start"
        android:textSize="20sp" />

</LinearLayout>
